<UserControl
    x:Class="GenshinImpact_Lanucher.ItemsControlDT.NitifyGameDT"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GenshinImpact_Lanucher.ItemsControlDT"
             mc:Ignorable="d" 
             xmlns:wpfui="http://schemas.lepo.co/wpfui/2022/xaml"
             HorizontalAlignment="Stretch"
             Background="Transparent"
             d:DesignHeight="450" d:DesignWidth="800">
    <wpfui:Card HorizontalAlignment="Stretch" Margin="10" Padding="5">
        <Grid>
            <Image Source="{Binding banner}" Stretch="UniformToFill"/>
        </Grid>
    </wpfui:Card>
</UserControl>
